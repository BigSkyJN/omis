<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="omis.tierdesignation.domain">
	<class entity-name="omis.tierdesignation.domain.TierLevel"
	       proxy="omis.tierdesignation.domain.TierLevel"
	       name="omis.tierdesignation.domain.impl.TierLevelImpl"
	       table="`TIER_LEVEL`">
		<id name="id" column="`ID`" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">`TIER_LEVEL_SEQ`</param>
			</generator>
		</id>
		<property name="name" type="java.lang.String" not-null="true" unique="true"
		          column="`NAME`"/>
		<property name="valid" type="yes_no" not-null="true" column="`VALID`"/>
		<property name="sortOrder" type="java.lang.Short" not-null="true"
		          column="`SORT_ORDER`"/>
	</class>
	<query name="findTierLevel"><![CDATA[
	  select tierLevel
	  from omis.tierdesignation.domain.TierLevel tierLevel
	  where
	  		tierLevel.name = :name
	]]></query>
	<query name="findTierLevelExcluding"><![CDATA[
	  select tierLevel
	  from omis.tierdesignation.domain.TierLevel tierLevel
	  where
	  		tierLevel.name = :name
	  and	
	  		tierLevel <> :excludedLevel
	]]></query>
</hibernate-mapping>