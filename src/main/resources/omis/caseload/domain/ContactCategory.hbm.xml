<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="omis.caseload.domain">
	<class entity-name="omis.caseload.domain.ContactCategory"
		proxy="omis.caseload.domain.ContactCategory" 
		name="omis.caseload.domain.impl.ContactCategoryImpl"
		table="`CONTACT_CATEGORY`">
		<id name="id" column="`ID`" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">`CONTACT_CATEGORY_SEQ`</param>
			</generator>
		</id>
		<property name="name" column="`NAME`" not-null="true" unique="true" />
		<component name="creationSignature">
			<many-to-one name="userAccount" column="`CREATE_ACCOUNT_ID`"
				not-null="true" />
			<property name="date" column="`CREATE_DATE`" not-null="true" />
		</component>
	</class>
	
	<query name="findAllContactCategories"><![CDATA[
		select category
			from omis.caseload.domain.ContactCategory category
			order by
			category.name
	]]></query>
</hibernate-mapping>