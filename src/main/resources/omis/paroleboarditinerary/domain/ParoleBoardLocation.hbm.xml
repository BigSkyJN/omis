<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="omis.paroleboarditinerary.domain">
	<class entity-name="omis.paroleboarditinerary.domain.ParoleBoardLocation"
			proxy="omis.paroleboarditinerary.domain.ParoleBoardLocation"
			name="omis.paroleboarditinerary.domain.impl.ParoleBoardLocationImpl"
			table="`PAROLE_BOARD_LOCATION`">
			<id name="id" column="`ID`" type="java.lang.Long">
				<generator class="sequence">
					<param name="sequence">`PAROLE_BOARD_LOCATION_SEQ`</param>
				</generator>
			</id>
			<many-to-one name="location" column="`LOCATION_ID`" entity-name="omis.location.domain.Location" not-null="true" />
			<property name="videoConferenceCapable" column="`VIDEO_CONFERENCE_CAPABLE`" type="yes_no" not-null="true" />
	</class>
</hibernate-mapping>